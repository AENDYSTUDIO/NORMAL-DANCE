{
  "version": 2,
  "env": {
    "NODE_ENV": "development",
    "DOMAIN": "dev.dnb1st.ru",
    "NEXT_PUBLIC_APP_URL": "https://dev.dnb1st.ru",
    "NEXT_PUBLIC_API_URL": "https://dev.dnb1st.ru/api",
    "NEXTAUTH_URL": "https://dev.dnb1st.ru",
    "NEXTAUTH_SECRET": "@nextauth_secret_dev",
    "DATABASE_URL": "@database_url_dev",
    "SOLANA_RPC_URL": "@solana_rpc_url_dev",
    "REDIS_URL": "@redis_url_dev",
    "LOG_LEVEL": "debug",
    "ENABLE_DEBUG": "true",
    "ENABLE_MOCK_DATA": "true",
    "HOT_RELOAD": "true",
    "CACHE_ENABLED": "false",
    "ANALYTICS_ENABLED": "false",
    "MONITORING_ENABLED": "false",
    "RATE_LIMITING_ENABLED": "false",
    "SSL_ENABLED": "false",
    "HSTS_ENABLED": "false",
    "SENTRY_DSN": "@sentry_dsn_dev",
    "NEXT_PUBLIC_SENTRY_DSN": "@sentry_dsn_dev",
    "VERCEL_ANALYTICS_ENABLED": "false",
    "SENTRY_USER": "@sentry_user_dev",
    "SENTRY_USER_EMAIL": "@sentry_user_email_dev"
  },
  "analytics": {
    "enabled": false,
    "performance": {
      "enabled": false,
      "coreWebVitals": {
        "enabled": false,
        "trackLCP": false,
        "trackFID": false,
        "trackCLS": false,
        "trackFCP": false,
        "trackTBT": false,
        "trackINP": false,
        "trackTTFB": false
      },
      "customMetrics": {
        "enabled": false,
        "trackPageLoad": false,
        "trackRouteChanges": false,
        "trackApiCalls": false,
        "trackUserInteractions": false
      }
    },
    "realUserMonitoring": {
      "enabled": false,
      "sampleRate": 0,
      "trackUserAgent": false,
      "trackViewport": false,
      "trackDeviceType": false,
      "trackConnectionType": false
    },
    "errorTracking": {
      "enabled": false,
      "trackJavaScriptErrors": false,
      "trackUnhandledRejections": false,
      "trackResourceErrors": false,
      "trackWebVitalsErrors": false
    },
    "events": {
      "enabled": false,
      "trackPageViews": false,
      "trackClicks": false,
      "trackFormSubmissions": false,
      "trackCustomEvents": false
    },
    "sampling": {
      "enabled": false,
      "sampleRate": 0,
      "sessionSampleRate": 0
    },
    "retention": {
      "dataRetentionDays": 1,
      "aggregateDataRetentionDays": 7
    },
    "privacy": {
      "anonymizeIP": false,
      "maskUserData": false,
      "respectDoNotTrack": false,
      "consentMode": "denied"
    }
  },
  "caching": {
    "maxAge": 0,
    "staleWhileRevalidate": 0
  },
  "compression": {
    "brotli": false,
    "gzip": true
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "trailingSlash": false,
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30,
      "memory": 512
    },
    "src/app/api/**/*.js": {
      "maxDuration": 30,
      "memory": 512
    }
  }
}